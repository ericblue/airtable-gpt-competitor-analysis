  "private": true,
  "proxy": "http://localhost:3000",
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-json-view": "^1.21.3",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider react-scripts start",
